{
    "name": "topdata-software-gmbh/php-util",
    "type": "library",
    "description": "Utility classes for php",
    "keywords": [
        "util",
        "utility"
    ],
    "homepage": "https://github.com/topdata-software-gmbh/php-util",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "suggest": {
        "symfony/var-dumper": "for UtilDebug",
        "phpfastcache/phpfastcache": "6.X for caching in UtilCurl",
        "eventviva/php-image-resize": "for image resizing",
        "jdorn/sql-formatter": "for UtilDebug::getSqlFormatted()",
        "swiftmailer/swiftmailer": "for mails",
        "tijsverkoyen/css-to-inline-styles": "for html mails with css",
        "kub-at/php-simple-html-dom-parser": "for scraping stuff",
        "coldume/imagecraft": "for resizing animated gifs"
    },
    "autoload": {
        "psr-4": {
            "TopdataSoftwareGmbH\\Util\\": "src/Util/",
            "TopdataSoftwareGmbH\\HelperClasses\\": "src/HelperClasses/"
        }
    },
    "version": "4.2.1"
}
